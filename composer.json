{
    "name": "qroques/resilient-data",
    "description": "A package to make data resilient",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Qroques\\ResilientData\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\unit\\": "tests/unit",
            "tests\\integration\\": "tests/integration"
        }
    },
    "authors": [
        {
            "name": "Quentin Roques"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.11",
        "friendsofphp/php-cs-fixer": "^3.58",
        "pedrotroller/php-cs-custom-fixer": "^2.33",
        "phpunit/phpunit": "^11.2"
    }
}
